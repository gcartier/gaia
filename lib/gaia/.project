;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: gaia                    documentation: {File :context "doc" "gaia.jml"} description-file: {File :context ".package"}
    (<Project-Directory>                                   directory: {Directory :context "src"}
      (<Project-Directory>                                 directory: {Directory :context "src" "gaia"}
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "application"}
          (<Jazz-File>       name: Gaia-Menubar            source: {File :context "src" "gaia" "application" "Gaia-Menubar.jazz"})
          (<Jazz-File>       name: Gaia-Status             source: {File :context "src" "gaia" "application" "Gaia-Status.jazz"})
          (<Jazz-File>       name: Gaia-Toolbar            source: {File :context "src" "gaia" "application" "Gaia-Toolbar.jazz"})
          (<Jazz-File>       name: Gaia-Toplevel           source: {File :context "src" "gaia" "application" "Gaia-Toplevel.jazz"})
          (<Jazz-File>       name: Gaia-Application        source: {File :context "src" "gaia" "application" "Gaia-Application.jazz"}))
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "profile"}
          (<Jazz-File>       name: Gaia-Profile            source: {File :context "src" "gaia" "profile" "Gaia-Profile.jazz"}))
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "search"}
          (<Jazz-File>       name: Gaia-Search             source: {File :context "src" "gaia" "search" "Gaia-Search.jazz"}))
        (<Jazz-File>         name: _gaia                   source: {File :context "src" "gaia" "_gaia.jazz"})
        (<Jazz-File>         name: autoload                source: {File :context "src" "gaia" "autoload.jazz"})
        (<Jazz-File>         name: run                     source: {File :context "src" "gaia" "run.jazz"})
        (<Project-File>                                    source: {File :context "src" "gaia" "install.scm"})
        (<Jazz-File>         name: services                source: {File :context "src" "gaia" "services.jazz"})
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "data"}
          (<Jazz-File>       name: bindings                source: {File :context "src" "gaia" "data" "bindings.jazz"})
          (<Jazz-File>       name: preferences             source: {File :context "src" "gaia" "data" "preferences.jazz"})
          (<Jazz-File>       name: workspaces              source: {File :context "src" "gaia" "data" "workspaces.jazz"}))
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "validate"}
          (<Jazz-File>       name: Gaia-Tester             source: {File :context "src" "gaia" "validate" "Gaia-Tester.jazz"})
          (<Jazz-File>       name: _validate               source: {File :context "src" "gaia" "validate" "_validate.jazz"})
          (<Jazz-File>       name: autoload                source: {File :context "src" "gaia" "validate" "autoload.jazz"}))
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "preference"}
          (<Jazz-File>       name: Gaia-Actions            source: {File :context "src" "gaia" "preference" "Gaia-Actions.jazz"})
          (<Jazz-File>       name: Gaia-Bindings           source: {File :context "src" "gaia" "preference" "Gaia-Bindings.jazz"})
          (<Jazz-File>       name: Gaia-Preferences        source: {File :context "src" "gaia" "preference" "Gaia-Preferences.jazz"}))
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "debugger"}
          (<Jazz-File>       name: _debugger               source: {File :context "src" "gaia" "debugger" "_debugger.jazz"})
          (<Jazz-File>       name: implementation          source: {File :context "src" "gaia" "debugger" "implementation.jazz"})
          (<Jazz-File>       name: stub                    source: {File :context "src" "gaia" "debugger" "stub.jazz"}))
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "live"}
          (<Jazz-File>       name: Live-Properties-Manager source: {File :context "src" "gaia" "live" "Live-Properties-Manager.jazz"})
          (<Jazz-File>       name: Live-Properties         source: {File :context "src" "gaia" "live" "Live-Properties.jazz"})
          (<Jazz-File>       name: Live-Style              source: {File :context "src" "gaia" "live" "Live-Style.jazz"})
          (<Jazz-File>       name: _live                   source: {File :context "src" "gaia" "live" "_live.jazz"}))
        (<Project-Directory>                               directory: {Directory :context "src" "gaia" "parse"}
          (<Jazz-File>       name: parcil                  source: {File :context "src" "gaia" "parse" "parcil.jazz"})))
      (<Project-Directory>                                 directory: {Directory :context "src" "world"}
        (<Project-Directory>                               directory: {Directory :context "src" "world" "debuggee"}
          (<Jazz-File>       name: stub                    source: {File :context "src" "world" "debuggee" "stub.jazz"}))))
    (<Project-Directory>                                   directory: {Directory :context "doc"}
      (<Project-File>                                      source: {File :context "doc" "gaia.jml"})))))
